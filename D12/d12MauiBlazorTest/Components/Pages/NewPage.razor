@page "/newpage"

<h3>Ny Sida</h3>

<p role="status">Nuvarande räknare: @currentCount</p>

<button class="btn btn-primary" @onclick="IncrementCount">Tryck här!</button>

@code {
    private int currentCount = 0;

    private void IncrementCount()
    {
        currentCount++;
    }
}
